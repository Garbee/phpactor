{
    "name": "dantleech/phpactor",
    "minimum-stability": "dev",
    "require": {
        "symfony/console": "~2.7|^3.0",
        "dantleech/worse-reflection": "dev-hotfixes",
        "phpbench/container": "^1.0",
        "sylius/registry": "^0.19.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "symfony/process": "^2.7|^3.0",
        "symfony/options-resolver": "^2.7|^3.0"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/dantleech/BetterReflection" }
    ],
    "autoload": {
        "psr-4": {
            "Phpactor\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Tests\\": "tests/",
            "Phpactor\\Benchmark\\": "benchmark/"
        }
    }
}
